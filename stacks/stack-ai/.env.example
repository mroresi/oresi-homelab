# AI Stack Environment Variables
# Copy this file to .env and update with your values

# OpenWebUI Configuration
# Generate a secure key with: openssl rand -hex 32
WEBUI_SECRET_KEY=your_64_char_hex_key_here # e.g. output of: openssl rand -hex 32
WEBUI_AUTH=true

# Optional: Custom Ollama settings
# OLLAMA_HOST=0.0.0.0
# OLLAMA_MODELS=/srv/ai_data/ollama/models

# Optional: Stable Diffusion settings
# SD_WEBUI_AUTH=username:password
# SD_COMMANDLINE_ARGS=--medvram --xformers
